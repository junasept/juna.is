{
	"name": "juna.is",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"astro": "astro",
		"check": "astro check && biome check",
		"check:fix": "astro check && biome check --write",
		"prepare": "husky"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/netlify": "^6.5.6",
		"astro": "^5.12.8",
		"typescript": "^5.9.2"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.3",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.4"
	},
	"lint-staged": {
		"*.{astro,js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
			"biome check --write --no-errors-on-unmatched"
		]
	}
}
